{
  "name": "arduino-uno-r4-minima",
  "version": "1.0.0",
  "author": "ArthurZheng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openblock-plugin/arduinoUnoR4Minima.git"
  },
  "type": "module",
  "scripts": {
    "dev": "obr dev",
    "build": "obr build",
    "lint": "eslint . --ext .js --fix"
  },
  "openblock": {
    "name": "Arduino Uno R4 Minima",
    "deviceId": "arduinoUnoR4Minima",
    "manufactor": "arduino",
    "learnMore": "https://docs.arduino.cc/hardware/uno-r4-minima",
    "type": "arduino",
    "iconURL": "./assets/arduinoUnoR4Minima.png",
    "description": {
      "formatMessage": {
        "id": "device.arduinoUnoR4Minima.description",
        "default": "Offers enhanced performance and expanded memory, maintaining compatibility with existing shields and accessories."
      }
    },
    "featured": true,
    "disabled": false,
    "bluetoothRequired": false,
    "serialportRequired": true,
    "defaultBaudRate": "9600",
    "internetConnectionRequired": false,
    "launchPeripheralConnectionFlow": true,
    "useAutoScan": false,
    "connectionIconURL": "./assets/arduinoUnoR4Minima-illustration.svg",
    "connectionSmallIconURL": "./assets/arduinoUnoR4Minima-small.svg",
    "main": "./src/arduinoUnoR4Minima.js",
    "translations": "./src/translations.js",
    "dependencies": {
        "toolchains": {
            "arduino-arduino-renesas_uno": "latest"
        }
    },
    "programMode": ["realtime"],
    "programLanguage": ["block", "cpp"],
    "tags": ["arduino"],
    "helpLink": "https://wiki.openblock.cc/general-hardware-guidelines/boards/arduino-uno-r4-minima"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-config-scratch": "^5.0.0"
  }
}
